<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">
    <objectAnimator android:propertyName="rotation"
        android:valueFrom="0f" android:valueTo="720f" android:valueType="floatType"
        android:duration="2000"/>
    <objectAnimator android:propertyName="x"
                    android:valueFrom="30dp" android:valueTo="200dp" android:valueType="floatType"
                    android:duration="1000"
        android:interpolator="@android:interpolator/anticipate_overshoot"/>
    <objectAnimator android:propertyName="y"
                    android:valueFrom="30dp" android:valueTo="200dp" android:valueType="floatType"
                    android:duration="1000"
                    android:interpolator="@android:interpolator/anticipate_overshoot"/>
</set>